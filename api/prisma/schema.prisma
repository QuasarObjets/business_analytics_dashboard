// Archivo de esquema de base de datos de Prisma

// Define la estructura de las tablas y las relaciones entre ellas

// Ejemplo de definici√≥n de una tabla de ejemplo
model Usuario {
  id        Int      @id @default(autoincrement())
  nombre    String
  email     String   @unique
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}